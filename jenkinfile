pipeline{
  agent any
    stages{
    stage('Clone code'){
      steps{
      echo "Clone the code" 
        git url:""
      }
      stage('Build'){
      steps{
      echo "Build the image" 
      }
        stage('Push to docekr hub'){
      steps{
      echo "Pushing the image to docker hub" 
      }
      
     }
    }
}
